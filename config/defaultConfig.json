{
  "gitRemote": {
    "username": "",
    "password": ""
  },
  "options": {
    "internal": {
      "portRange": [8000, 9000]
    },
    "proxy": {
      "port": 80,
      "hostnames": [
        {"domain": "localhost", "secure": false, "service": "author/repo"}
      ]
    }
  },
  "services": {
    "author/repo": {
      "commands": {
        "start": "npm start",
        "install": "npm install",
        "build": "npm run build"
      },
      "args": {
        "port": "-p"
      },
      "env": {}
    }
  }
}